<div class="row">
  <div class="col-md-4">
    <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => {:class => "well"}, :builder => BoostrapFormBuilder ) do |f| %>
    <fieldset>
      <legend>Sign up</legend>

      <%= f.text_field :first_name %>
      <%= f.text_field :last_name %>
      <%= f.text_field :profile_name %>
      <%= f.email_field :email %>
      <%= f.password_field :password %>
      <%= f.password_field :password_confirmation %>

      <%= button_tag "Sign up", class: 'btn btn-primary' %>
    </fieldset>
    <% end %>
    
    <%= render "devise/shared/links" %>
  </div>
  <div class="col-md-8">
    <h2>Sign up 'cause is cool.</h2>
  </div>
</div>